        "ctx7sk-a72c195a-c1b2-4ffb-a2bd-e1f5df42956e"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "MCP_DOCKER": {
      "command": "docker",
      "args": [
        "mcp",
        "gateway",
        "run"
      ],
      "disabled": false,
      "autoApprove": [
        "create_repository",
        "search_repositories",
        "get_me",
        "get_file_contents",
        "create_issue",
        "obsidian_append_content"
      ],
      "disabledTools": []
    }
